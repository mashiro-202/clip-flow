{
  "name": "clip-flow",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "main": "electron/main.cjs",
  "build": {
    "appId": "com.example.myreactapp",
    "win": {
      "target": "nsis",
      "icon": "public/favicon.ico"
    },
    "files": [
    "dist/**/**",
    "electron/main.cjs",
    "node_modules/**/**",
    "package.json"
    ],
  "nsis": {
    "uninstallDisplayName": "Uninstall React App",
    "license": "license.txt",
    "oneClick": "false",
    "allowToChangeInstallationDirectory": "true"
    },
    "directories": {
    "output": "build",
    "buildResources": "dist"
    }
  },
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "lint": "eslint .",
    "preview": "vite preview"
  },
  "dependencies": {
    "react": "^19.1.0",
    "react-dom": "^19.1.0"
  },
  "devDependencies": {
    "electron": "^36.1.0",
    "electron-builder": "^26.0.12",
    "@eslint/js": "^9.25.0",
    "@types/react": "^19.1.2",
    "@types/react-dom": "^19.1.2",
    "@vitejs/plugin-react": "^4.4.1",
    "eslint": "^9.25.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.19",
    "globals": "^16.0.0",
    "vite": "^6.3.5"
  }
}
